<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.done.LogIn_Admin">
   <children>
      <ImageView fitHeight="360.0" fitWidth="532.0" layoutX="54.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@IMG/undraw_Fingerprint_re_uf3f.png" />
         </image>
      </ImageView>
      <TextField fx:id="Username_txt" layoutX="90.0" layoutY="115.0" promptText="Username" />
      <Label fx:id="username_lbl" layoutX="7.0" layoutY="116.0" text="Username">
         <font>
            <Font name="Alef Regular" size="17.0" />
         </font></Label>
      <Label fx:id="password_lbl" layoutX="10.0" layoutY="239.0" text="Password">
         <font>
            <Font name="Alef Regular" size="17.0" />
         </font></Label>
      <Button fx:id="back_btn" layoutX="124.0" layoutY="307.0" mnemonicParsing="false" onAction="#Back_pressed" prefHeight="48.0" prefWidth="85.0" style="-fx-background-color: #793bff;" text="Back" textFill="WHITE">
         <font>
            <Font name="Alef Regular" size="17.0" />
         </font></Button>
      <PasswordField fx:id="Password_txt" layoutX="90.0" layoutY="238.0" promptText="Password" />
      <Button fx:id="Login_btn" layoutX="27.0" layoutY="307.0" mnemonicParsing="false" onAction="#Login_Pressed" prefHeight="48.0" prefWidth="86.0" style="-fx-background-color: #793bff;"  textFill="WHITE" text="Login">
         <font>
            <Font name="Alef Regular" size="17.0" />
         </font></Button>
   </children>
</AnchorPane>
